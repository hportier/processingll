package ch.ecal.processing.hello;

import processing.core.PApplet;

public class UseHelloProcessing extends PApplet {

	private static final long serialVersionUID = 1L;
	
	public void setup() {
		size(200, 200);
		background(0);
		
		int x = 0;
		int y = 0;
		
	}
	
	public void draw() {
		ellipse(x, y, 30, 30);
	}

}

