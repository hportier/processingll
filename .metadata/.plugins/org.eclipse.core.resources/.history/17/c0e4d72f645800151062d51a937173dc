package ch.ecal.processing.threeD;

import processing.core.PApplet;

public class Use3D extends PApplet {
	private static final long serialVersionUID = 1L;
	
	public void setup() {
		size(300, 300, P3D);
	}
	
	
	public void draw() {
		
		background(0);
		lights();
		pushMatrix();
		translate(width/2, height/2);
		rotateY(PI/6);
		scale(100);
		
		translate(0,1, 0);
		box(1);
		popMatrix();
	}

}

